#: rayforge/package_mgr/update_cmd.py
msgid "Checking for package updates..."
msgstr "Vérification des mises à jour des paquets..."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "An update is available for {name}."
msgstr "Une mise à jour est disponible pour {name}."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Updates are available for {name1} and {name2}."
msgstr "Des mises à jour sont disponibles pour {name1} et {name2}."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Updates are available for {name1}, {name2}, and {num} others."
msgstr ""
"Des mises à jour sont disponibles pour {name1}, {name2} et {num} autres."

#: rayforge/package_mgr/update_cmd.py
msgid "Install All"
msgstr "Tout installer"

#: rayforge/package_mgr/update_cmd.py
msgid "Package updates found."
msgstr "Des mises à jour de paquets ont été trouvées."

#: rayforge/package_mgr/update_cmd.py
msgid "Packages are up to date."
msgstr "Les paquets sont à jour."

#: rayforge/package_mgr/update_cmd.py
msgid "Update check failed."
msgstr "La vérification des mises à jour a échoué."

#: rayforge/package_mgr/update_cmd.py
msgid "Installing package updates..."
msgstr "Installation des mises à jour des paquets..."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Package successfully updated."
msgid_plural "{num} packages successfully updated."
msgstr[0] "Paquet mis à jour avec succès."
msgstr[1] "{num} paquets mis à jour avec succès."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "{num_s} packages updated, {num_f} failed."
msgstr "{num_s} paquets mis à jour, {num_f} ont échoué."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Failed to update package."
msgid_plural "Failed to update {num} packages."
msgstr[0] "Échec de la mise à jour du paquet."
msgstr[1] "Échec de la mise à jour de {num} paquets."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Finished with {num_failed} errors."
msgstr "Terminé avec {num_failed} erreurs."

#: rayforge/package_mgr/update_cmd.py
msgid "All package updates installed!"
msgstr "Toutes les mises à jour de paquets sont installées !"


#: rayforge/package_mgr/update_cmd.py
msgid "Checking for package updates..."
msgstr "Comprobando actualizaciones de paquetes..."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "An update is available for {name}."
msgstr "Hay una actualización disponible para {name}."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Updates are available for {name1} and {name2}."
msgstr "Hay actualizaciones disponibles para {name1} y {name2}."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Updates are available for {name1}, {name2}, and {num} others."
msgstr "Hay actualizaciones disponibles para {name1}, {name2} y otros {num}."

#: rayforge/package_mgr/update_cmd.py
msgid "Install All"
msgstr "Instalar todo"

#: rayforge/package_mgr/update_cmd.py
msgid "Package updates found."
msgstr "Se encontraron actualizaciones de paquetes."

#: rayforge/package_mgr/update_cmd.py
msgid "Packages are up to date."
msgstr "Los paquetes están actualizados."

#: rayforge/package_mgr/update_cmd.py
msgid "Update check failed."
msgstr "La comprobación de actualizaciones falló."

#: rayforge/package_mgr/update_cmd.py
msgid "Installing package updates..."
msgstr "Instalando actualizaciones de paquetes..."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Package successfully updated."
msgid_plural "{num} packages successfully updated."
msgstr[0] "Paquete actualizado correctamente."
msgstr[1] "{num} paquetes actualizados correctamente."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "{num_s} packages updated, {num_f} failed."
msgstr "{num_s} paquetes actualizados, {num_f} fallaron."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Failed to update package."
msgid_plural "Failed to update {num} packages."
msgstr[0] "Error al actualizar el paquete."
msgstr[1] "Error al actualizar {num} paquetes."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Finished with {num_failed} errors."
msgstr "Finalizado con {num_failed} errores."

#: rayforge/package_mgr/update_cmd.py
msgid "All package updates installed!"
msgstr "¡Todas las actualizaciones de paquetes instaladas!"


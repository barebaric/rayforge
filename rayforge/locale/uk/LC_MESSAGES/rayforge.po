#, python-brace-format
msgid "Updates are available for {name1}, {name2}, and {num} others."
msgstr "Доступні оновлення для {name1}, {name2} та ще {num}."

#: rayforge/package_mgr/update_cmd.py
msgid "Install All"
msgstr "Встановити все"

#: rayforge/package_mgr/update_cmd.py
msgid "Package updates found."
msgstr "Знайдено оновлення пакетів."

#: rayforge/package_mgr/update_cmd.py
msgid "Packages are up to date."
msgstr "Пакети актуальні."

#: rayforge/package_mgr/update_cmd.py
msgid "Update check failed."
msgstr "Помилка перевірки оновлень."

#: rayforge/package_mgr/update_cmd.py
msgid "Installing package updates..."
msgstr "Встановлення оновлень пакетів..."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Package successfully updated."
msgid_plural "{num} packages successfully updated."
msgstr[0] "Пакет успішно оновлено."
msgstr[1] "{num} пакети успішно оновлено."
msgstr[2] "{num} пакетів успішно оновлено."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "{num_s} packages updated, {num_f} failed."
msgstr "{num_s} пакетів оновлено, {num_f} з помилкою."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Failed to update package."
msgid_plural "Failed to update {num} packages."
msgstr[0] "Не вдалося оновити пакет."
msgstr[1] "Не вдалося оновити {num} пакети."
msgstr[2] "Не вдалося оновити {num} пакетів."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Finished with {num_failed} errors."
msgstr "Завершено з {num_failed} помилками."

#: rayforge/package_mgr/update_cmd.py
msgid "All package updates installed!"
msgstr "Всі оновлення пакетів встановлено!"


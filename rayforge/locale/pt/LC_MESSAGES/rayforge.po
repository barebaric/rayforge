#: rayforge/package_mgr/update_cmd.py
msgid "Checking for package updates..."
msgstr "A verificar atualizações de pacotes..."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "An update is available for {name}."
msgstr "Está disponível uma atualização para {name}."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Updates are available for {name1} and {name2}."
msgstr "Estão disponíveis atualizações para {name1} e {name2}."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Updates are available for {name1}, {name2}, and {num} others."
msgstr "Estão disponíveis atualizações para {name1}, {name2} e outros {num}."

#: rayforge/package_mgr/update_cmd.py
msgid "Install All"
msgstr "Instalar Tudo"

#: rayforge/package_mgr/update_cmd.py
msgid "Package updates found."
msgstr "Foram encontradas atualizações de pacotes."

#: rayforge/package_mgr/update_cmd.py
msgid "Packages are up to date."
msgstr "Os pacotes estão atualizados."

#: rayforge/package_mgr/update_cmd.py
msgid "Update check failed."
msgstr "A verificação de atualizações falhou."

#: rayforge/package_mgr/update_cmd.py
msgid "Installing package updates..."
msgstr "A instalar atualizações de pacotes..."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Package successfully updated."
msgid_plural "{num} packages successfully updated."
msgstr[0] "Pacote atualizado com sucesso."
msgstr[1] "{num} pacotes atualizados com sucesso."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "{num_s} packages updated, {num_f} failed."
msgstr "{num_s} pacotes atualizados, {num_f} falharam."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Failed to update package."
msgid_plural "Failed to update {num} packages."
msgstr[0] "Falha ao atualizar o pacote."
msgstr[1] "Falha ao atualizar {num} pacotes."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Finished with {num_failed} errors."
msgstr "Concluído com {num_failed} erros."

#: rayforge/package_mgr/update_cmd.py
msgid "All package updates installed!"
msgstr "Todas as atualizações de pacotes foram instaladas!"


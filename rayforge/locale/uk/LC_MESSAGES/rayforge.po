#: rayforge/package_mgr/update_cmd.py
msgid "Checking for package updates..."
msgstr "Перевірка оновлень пакетів..."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "An update is available for {name}."
msgstr "Доступне оновлення для {name}."

#: rayforge/package_mgr/update_cmd.py
#, python-brace-format
msgid "Updates are available for {name1} and {name2}."
msgstr "Доступні оновлення для {name1} та {name2}."

